{
    "defaultSeverity": "error",
    "extends": [
        "tslint:recommended"
    ],
    "jsRules": {},
    "rules": {
        "arrow-parens": false,
        "interface-name": [
            true,
            "never-prefix"
        ],
        "no-console": false,
        "no-empty-interface": false,
        "object-literal-sort-keys": false,
        "object-literal-shorthand": [
            true,
            "never"
        ]
    },
    "rulesDirectory": []
}
